generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model users {
  id            String  @id @default(auto()) @map("_id") @db.ObjectId
  dateRemaining Int
  email         String
  isFreeTrial   Boolean
  password      String
  username      String
}
